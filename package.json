{
  "name": "superalgos",
  "version": "1.6.0",
  "description": "Superalgos is a free, open-source crypto trading bot",
  "author": {
    "name": "Superalgos Contributors"
  },
  "scripts": {
    "serve": "webpack-dev-server --mode development --config ./Dashboards/webpack.config.js",
    "build": "webpack build --config ./Dashboards/webpack.config.js",
    "dist": "electron-builder -c build/electron-builder.json",
    "installSocialTradingReact": "cd ./Social-Trading/React-UI && npm ci",
    "lintAll": "eslint . --ext .js",
    "lintFile": "eslint --ext .js",
    "postsetup": "npm start",
    "prepare": "husky install",
    "presetup": "git checkout develop && git pull upstream develop",
    "setup": "node setup noShortcuts",
    "setupWithTf": "node setupWithTf noShortcuts",
    "socialTradingAppBuild": "cd ./Social-Trading/Vue-UI && vite build",
    "socialTradingAppDev": "cd ./Social-Trading/Vue-UI && vite dev",
    "socialTradingAppPreview": "cd ./Social-Trading/Vue-UI && vite preview",
    "start": "node platform minMemo noBrowser",
    "startLocalSocialTradingReact": "npm run startNetwork && cd ./Social-Trading/React-UI && npm run startSocialTradingReactBackend && npm run startSocialTradingReactFrontend",
    "startNetwork": "node network",
    "startSocialTrading": "node socialTrading",
    "startSocialTradingReact": "cd ./Social-Trading/React-UI && npm run startSocialTradingReactBackend && npm run startSocialTradingReactFrontend",
    "unitTest": "jest --silent",
    "unitTest:coverage": "jest --silent --coverage",
    "unitTest:debug": "DEBUG=nock.* jest",
    "migration-init": "knex init",
    "migration-make": "knex migrate:make"
  },
  "main": "manageApps.js",
  "dependencies": {
    "@bundled-es-modules/axios": "^0.27.2",
    "@discordjs/rest": "^2.4.0",
    "@octokit/plugin-retry": "^7.1.2",
    "@octokit/rest": "^21.0.2",
    "@reduxjs/toolkit": "^2.2.7",
    "@slack/web-api": "^7.5.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "bootstrap": "*",
    "bootstrap-vue": "*",
    "boxen": "^8.0.1",
    "ccxt": "^4.4.6",
    "chalk": "^5.3.0",
    "child_process": "^1.0.2",
    "cli-table3": "^0.6.5",
    "date-fns": "^4.1.0",
    "discord-api-types": "^0.37.100",
    "discord.js": "^14.16.2",
    "dotenv": "16.4.5",
    "dotenv-expand": "11.0.6",
    "ethereumjs-common": "^1.5.2",
    "ethereumjs-tx": "^2.1.2",
    "ethers": "^6.13.2",
    "http": "0.0.1-security",
    "https": "^1.0.0",
    "hyperquest": "^2.1.3",
    "ip": "^2.0.1",
    "jsdom": "^25.0.1",
    "knex": "^3.1.0",
    "lookpath": "^1.2.2",
    "ndjson": "^2.0.0",
    "node-fetch": "^3.3.2",
    "node-static": "^0.7.11",
    "npm": ">=10.8.3",
    "open": "^10.1.0",
    "pako": "^2.1.0",
    "path": "^0.12.7",
    "pg": "^8.13.0",
    "pm2": "^5.4.2",
    "simple-git": "^3.27.0",
    "sqlite3": "^5.1.7",
    "telegraf": "^4.16.3",
    "twitter-api-v2": "^1.17.2",
    "util": "^0.12.5",
    "vader-sentiment": "^1.1.3",
    "vite": "^5.4.7",
    "vue": "^3.5.8",
    "vue-router": "^4.4.5",
    "web3": "^4.13.0",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0",
    "workbox-webpack-plugin": "^7.1.0",
    "ws": "^8.18.0",
    "xlsx": "^0.18.5",
    "yargs": "^17.7.2"
  },
  "optionalDependencies": {
    "@tensorflow/tfjs-node": "^4.21.0",
    "electron": "^32.1.2",
    "electron-builder": "^25.0.5",
    "electron-updater": "^6.3.4"
  },
  "devDependencies": {
    "axios": "^1.7.7",
    "css-loader": "^7.1.2",
    "eslint": "^9.11.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "nock": "^13.5.5",
    "vue-cli-plugin-axios": "~0.0.4",
    "vue-loader": "^17.4.2",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^0.1.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "transform": {
      "^.+\\.[m|t|j]s?$": "babel-jest"
    },
    "testMatch": [
      "**/.tests/**/?(*.)(test).js?(x)",
      "**/.tests/?(*.)(test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "node_modules"
    ],
    "coverageDirectory": "<rootDir>/.testsCoverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/Launch-Scripts/*.ico",
      "<rootDir>/Launch-Scripts/*.sh"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  },
  "bin": {
    "superalgos": "manageApps.js"
  },
  "homepage": "https://www.superalgos.org/",
  "license": "Apache License 2.0"
}
